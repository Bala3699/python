import numpy as np
a=np.array([1,2,3,4,5,6])
print(a)
print(type(a))

#0 dia
a=np.array(45)
print(a)

#1 dia
a1=np.array([11,13,12,15,14])
print(a1)

#2 dia
a2=np.array([[1,2,3,4,5],[6,5,4,8,7]])
print(a2)

#3 dia
a3=np.array([[1,2,3,4,5],[6,5,4,8,7],[0,2,3,4,5]])
print(a3)

print(a.ndim)
print(a1.ndim)
print(a2.ndim)
print(a3.ndim)


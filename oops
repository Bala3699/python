#without arguments

class sim:
    def add(self):
        return self.a+self.b
    def dis(self):
        print("rcb is won")
s=sim()
s.a=10
s.b=20
print(s.add())
print(s.dis())
        
    
#with arguments


class sim:
    def add(self,r):
        return 3.14*self.r*self.r
    def dis(self,r):
        self.r=r
c=sim()
a=c.dis(10)
print(c.add(a))        
